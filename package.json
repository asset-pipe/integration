{
  "name": "integration",
  "version": "1.0.0",
  "description": "Integration verification module",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint:format": "eslint --fix .",
    "lint": "eslint .",
    "precommit": "lint-staged",
    "cm": "git-cz",
    "readme": "projectz compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asset-pipe/integration.git"
  },
  "keywords": [
    "integration",
    "test",
    "asset-pipe"
  ],
  "author": "Richard Walker <digitalsadhu@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/asset-pipe/integration/issues"
  },
  "homepage": "https://github.com/asset-pipe/integration#readme",
  "devDependencies": {
    "@google-cloud/storage": "^1.4.0",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.9.0",
    "eslint-config-finn": "^3.0.0",
    "eslint-config-finn-prettier": "^3.0.1",
    "http-proxy": "^1.16.2",
    "husky": "^0.14.3",
    "jest": "^22.1.1",
    "lint-staged": "^10.1.6",
    "prettier": "^1.7.4",
    "projectz": "^1.4.0",
    "supertest": "^3.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "clearMocks": true,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "setupTestFrameworkScriptFile": "./jest.setup.js"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix --config ./.eslintrc",
      "git add"
    ],
    "{package.json,README.md,LICENSE.md}": [
      "projectz compile",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "badges": {
    "list": [
      "travisci",
      "daviddm",
      "daviddmdev"
    ]
  },
  "maintainers": [],
  "contributors": [
    "Richard Walker (https://github.com/digitalsadhu)"
  ],
  "dependencies": {
    "@asset-pipe/client": "*",
    "@asset-pipe/css-reader": "*",
    "@asset-pipe/css-writer": "*",
    "@asset-pipe/js-reader": "*",
    "@asset-pipe/js-writer": "*",
    "@asset-pipe/server": "*",
    "@asset-pipe/sink-fs": "*",
    "@asset-pipe/sink-gcs": "*",
    "@asset-pipe/sink-mem": "*",
    "express": "^4.16.2",
    "ext-name": "^5.0.0"
  }
}
